<?xml version="1.0" encoding="utf-8"?>
<show_control>

  <!-- This is the one special sound used by Example_03.  -->
  <sounds>
    <version>1.0</version>

    <!-- The background sound is played from curtain open
	 to curtain close.  -->
    <sound>
      <name>background music</name>
      <wav_file_name>background_music.wav</wav_file_name>
      <designer_volume_level>0.33</designer_volume_level>
      <designer_pan>0.00</designer_pan>
    </sound>
    
    <routing></routing>
  </sounds>
</show_control>
